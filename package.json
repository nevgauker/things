{
  "name": "things-web",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "node -e \"if(process.env.VERCEL_ENV==='production'){require('child_process').execSync('prisma migrate deploy',{stdio:'inherit'})}\" && next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "6.18.0",
    "@tanstack/react-query": "5.90.5",
    "axios": "1.13.1",
    "bcryptjs": "2.4.3",
    "cloudinary": "1.41.3",
    "jsonwebtoken": "9.0.2",
    "next": "15.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "@prisma/extension-accelerate": "1.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "3.0.0",
    "@types/jsonwebtoken": "9.0.10",
    "@types/node": "20.19.24",
    "@types/react": "19.2.2",
    "autoprefixer": "10.4.21",
    "postcss": "8.5.6",
    "prisma": "6.18.0",
    "tailwindcss": "3.4.18",
    "typescript": "5.9.3"
  }
}
